type Query {
    users(id: Int): [User]!
    clockActions(userId: Int): [ClockAction]!
    lastClockAction(userId: Int): ClockAction!
}
type Mutation {
    user(name: String!) : User!
    clockIn(userId: Int!, desc:String): ClockAction
    clockOut(userId: Int!, desc:String): ClockAction
}

type User {
    id: ID!
    name: String!
    clockActions(type: Int, containsDesc: String) : [ClockAction]
}

type ClockAction {
    id: ID!
    type: Int!
    desc: String
    timestamp: String!
}